  #endif
 
  string str;
  cin >> str;
  string str2=""; 
  for(int i=0;i<str.length();){
          if(str[i] == '.'){
            str2 += '0';
            i += 1;
          }
          if(str[i] == '-' && str[i+1] == '.'){
            str2 += '1';
            i += 2;
          }
          if(str[i] == '-' && str[i+1] == '-'){
            str2 += '2';
            i += 2;
          }
  }
  cout << str2 << "\n";
  
 
 
}
                                                    
